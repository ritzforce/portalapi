{
  "name": "customerportal",
  "version": "1.0.0",
  "description": "Todd's Customer Portal",
  "main": "src/main.js",
  "homepage": "sfdcguy.herokuapp.com",
  "author": "sfdc Guy",
  "license": "Private",
  "keywords": [
    "Portal"
  ],
  "scripts": {
  
  },
  "standard": {
    "globals": [
      "__DEV__"
    ],
    "plugins": [
      "react"
    ],
    "rules": {
      "react/jsx-no-bind": 2,
      "react/no-did-update-set-state": 2,
      "react/no-unknown-property": 2,
      "react/prop-types": 2
    }
  },
  "betterScripts": {
    "start": {
      "command": "nodemon server.development",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config webpack.config.production.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:start": {
      "command": "node server.production",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "redux-immutable-state-invariant": "^1.2.4",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.7.1"
  },
  "dependencies": {
    "autoprefixer": "^6.0.2",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "babel-runtime": "^6.5.0",
    "better-npm-run": "0.0.7",
    "body-parser": "^1.15.2",
    "bootstrap-loader": "^1.0.10",
    "bootstrap-sass": "^3.3.6",
    "composable-middleware": "^0.3.0",
    "css-loader": "^0.19.0",
    "express": "^4.12.3",
    "express-jwt": "^5.1.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.9.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.2.1",
    "jwt-decode": "^2.1.0",
	"jsforce":"^1.7.1",
